<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">


        <record id="ws_config_settings_id" model="res.config.settings">
            <field name="company_id" ref="base.main_company"/>

            <field name="group_product_variant">1</field>
            <field name="module_sale_product_configurator">1</field>
            <field name="portal_confirmation_pay">1</field>
            <field name="group_stock_adv_location">1</field>
            <field name="group_uom">1</field>
            <field name="group_product_pricelist">1</field>
            <field name="group_sale_delivery_address">1</field>



        </record>
        <record id="stock.warehouse0" model="stock.warehouse">
            <field name="delivery_steps">pick_pack_ship</field>
        </record>
        <function model="res.config.settings" name="execute">
            <value model="res.config.settings"
                   search="[('id', '=', ref('ws_config_settings_id'))]"/>
        </function>
    </data>
</odoo>